{
  "rate_limiting": {
    "window_ms": 900000,
    "max_requests": 100,
    "skip_successful_requests": false,
    "skip_failed_requests": false
  },
  "cors": {
    "origin": "http://localhost:3000",
    "credentials": true,
    "methods": [
      "GET",
      "POST",
      "PUT",
      "DELETE",
      "OPTIONS"
    ],
    "allowed_headers": [
      "Content-Type",
      "Authorization"
    ]
  },
  "helmet": {
    "content_security_policy": {
      "directives": {
        "default_src": [
          "'self'"
        ],
        "script_src": [
          "'self'",
          "'unsafe-inline'"
        ],
        "style_src": [
          "'self'",
          "'unsafe-inline'"
        ],
        "img_src": [
          "'self'",
          "data:",
          "https:"
        ],
        "connect_src": [
          "'self'",
          "https://api.razorpay.com"
        ]
      }
    }
  }
}